<Window x:Class="ControlSystemViewer.PopupWindows.TroubleshootingPopupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlSystemViewer.PopupWindows"
        xmlns:help="clr-namespace:ControlSystemViewer.Views.Menu_Help"
        mc:Ignorable="d" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Title="{DynamicResource TROUBLESHOOTING}" Height="600" Width="800">
    <Grid>
        <help:Troubleshooting x:Name="Troubleshooting"/>
    </Grid>
</Window>
