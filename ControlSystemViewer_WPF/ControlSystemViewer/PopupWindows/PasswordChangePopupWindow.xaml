<Window x:Class="ControlSystemViewer.PopupWindows.PasswordChangePopupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlSystemViewer.PopupWindows"
        xmlns:sys="clr-namespace:ControlSystemViewer.Views.Menu_System"
        mc:Ignorable="d" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Title="{DynamicResource PASSWORD_CHANGE}" Height="545" Width="380">
    <Grid>
        <sys:PasswordChange x:Name="PasswordChange"/>
    </Grid>
</Window>
