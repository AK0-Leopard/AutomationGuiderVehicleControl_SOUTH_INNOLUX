<Window x:Class="ControlSystemViewer.PopupWindows.AddCommandPopupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlSystemViewer.PopupWindows"
        xmlns:sub="clr-namespace:ControlSystemViewer.Views.Menu_Operation.Sub"
        mc:Ignorable="d" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Title="{DynamicResource ADD_CMD}" Height="410" Width="510">
    <Grid>
        <sub:AddCommand x:Name="AddCommand"/>
    </Grid>
</Window>
