<Window x:Class="ControlSystemViewer.PopupWindows.TipMessagePopupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlSystemViewer.PopupWindows"
        xmlns:tipmsg="clr-namespace:ControlSystemViewer.Views.Menu_TipMessage"
        mc:Ignorable="d" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Title="{DynamicResource TIP_MESSAGE}" Height="500" Width="1000">
    <Grid>
        <tipmsg:TipMessage x:Name="TipMessage"/>
    </Grid>
</Window>
